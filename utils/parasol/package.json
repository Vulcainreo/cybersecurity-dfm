{
  "name": "parasol",
  "version": "0.3.0",
  "description": "A network graph exploration tool",
  "scripts": {
    "start": "cross-env NODE_ENV=api node docs/server.js",
    "lint": "eslint src",
    "webpack": "webpack --colors --progress --bail",
    "build": "cross-env NODE_ENV=production npm run webpack",
    "build:stage": "cross-env NODE_ENV=stage npm run webpack",
    "build:test": "cross-env NODE_ENV=test npm run webpack",
    "build:qa": "cross-env NODE_ENV=testing npm run webpack",
    "build:qa-manual": "cross-env NODE_ENV=qa-manual npm run webpack",
    "build:develop": "cross-env NODE_ENV=develop npm run webpack",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "docs-parasol": "rm build/js/main*.js; yarn build; cp build/js/parasol.js docs/assets/js/",
    "docs-sass": "sass docs/assets/sass/material-kit.scss > docs/assets/css/material-kit.css",
    "boilerplate-parasol": "git checkout boilerplate_release; cp build/js/parasol.js assets/js/parasol.js; git pull; git commit -am 'update parasol.js'; git push; git checkout master"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alx/parasol.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "mobx",
    "starter-kit"
  ],
  "author": "Alexandre Girard <git@alexgirard.com> (http://github.com/alx)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alx/parasol/issues"
  },
  "homepage": "http://alx.github.com/parasol",
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "@kadira/storybook-addon-knobs": "^1.7.1",
    "@kadira/storybook-addons": "^1.3.0",
    "assets-webpack-plugin": "^3.5.1",
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "9.0.1",
    "eslint-plugin-import": "^1.7.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-react": "^5.1.1",
    "html-webpack-plugin": "^2.28.0",
    "react-hot-loader": "^3.0.0-beta.2",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  },
  "dependencies": {
    "aframe": "^0.7.1",
    "aframe-meshline-component": "https://github.com/alx/aframe-meshline-component.git#unicode-fix",
    "aframe-react": "^4.3.0",
    "async": "^2.6.0",
    "axios": "^0.16.1",
    "bn.js": "^4.11.8",
    "chart.js": "^2.7.1",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.0",
    "deepdetect-js": "^0.1.5",
    "eslint-loader": "^1.7.1",
    "eslint-watch": "^3.1.0",
    "eth-block-tracker": "^2.2.2",
    "ethjs": "^0.3.0",
    "ethjs-filter": "^0.1.5",
    "ethjs-provider-http": "^0.1.6",
    "file-saver": "^1.3.3",
    "jss": "^6.3.0",
    "material-ui": "^0.19.4",
    "material-ui-upload": "^1.0.0",
    "mobx": "^3.1.0",
    "mobx-react": "^4.1.0",
    "mobx-react-form": "^1.20.2",
    "moment": "^2.17.1",
    "rc-slider": "^7.0.2",
    "rc-tooltip": "^3.4.3",
    "react": "^15.1.0",
    "react-addons-shallow-compare": "^15.5.2",
    "react-chartjs": "^0.8.0",
    "react-chartjs-2": "^2.6.4",
    "react-chartjs2": "^1.1.2",
    "react-dom": "^15.1.0",
    "react-sigma": "^1.2.17",
    "react-spinner-material": "^1.0.11",
    "react-tap-event-plugin": "^2.0.1",
    "three": "^0.89.0",
    "throttle-debounce": "^1.0.1"
  }
}
